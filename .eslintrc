{
  "extends": "@antfu",
  "rules": {
    "vue/multiline-html-element-content-newline": [
      "error",
      {
        "ignoreWhenEmpty": true,
        "ignores": [
          "pre",
          "textarea"
        ],
        "allowEmptyLines": false
      }
    ],
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": {
          "max": 3
        },
        "multiline": {
          "max": 1
        }
      }
    ]
  }
}
